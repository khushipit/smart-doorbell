<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.doorbell">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/filename"
        android:label="@string/app_name"
        android:roundIcon="@drawable/filename"
        android:supportsRtl="true"
        android:theme="@style/Theme.Doorbell">
        <activity android:name=".smoke"></activity>
        <activity android:name=".light_ldr" />
        <activity android:name=".motion" />
        <activity android:name=".fire" />
        <activity android:name=".temprature" />
        <activity android:name=".editprofile" />
        <activity android:name=".accountme" />
        <activity android:name=".security" />
        <activity android:name=".Homepage" />
        <activity android:name=".forgotpass" />
        <activity android:name=".Login_page" />
        <activity android:name=".registeration" />
        <activity
            android:name=".onboarding"
            android:theme="@style/Theme.Intro" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>